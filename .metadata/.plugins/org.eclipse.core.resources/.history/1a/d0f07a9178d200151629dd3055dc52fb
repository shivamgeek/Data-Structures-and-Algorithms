package com.stack;

public class stack {

	int top=-1,max=10;
	char arr[]=new char[max];
	
	void push(char data){
		top++;
		if(top>=max){
		top--;
		System.out.println("stack overflow !!!");
		}
		else{
			arr[top]=data;
		}
	}
	
	void pop(){
		top--;
		if(top<0){
			top++;
			System.out.println("stack underflow !!!");
		}else{
			System.out.println("element popped");
		}
		
		
	}
	
	void display(){
	 for(int i=0;i<=top;i++){
		 System.out.println(arr[i]);
	 }
		
	}
	 
	char peek(){
		if(top<0){
			System.out.println("stack empty");
			return (Character) null;
		}
		else{
			return arr[top];
		}
	}
	
	
	
	
	
	
}
